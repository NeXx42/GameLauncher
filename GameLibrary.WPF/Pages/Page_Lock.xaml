<Page x:Class="GameLibary.Pages.Page_Lock"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GameLibary.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page_Lock">

    <Grid>
        <Label Content="Login" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,25,0,0" FontSize="24"/>
        <TextBox x:Name="inp_password" VerticalAlignment="Top" Margin="0,100,0,0" HorizontalAlignment="Center" Width="400" Height="50"/>
        <Button x:Name="btn_login" Content="Login" Margin="0,170,0,0" Width="100" Height="30" HorizontalAlignment="Center" VerticalAlignment="Top"/>
    </Grid>
</Page>
