<Window xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="700" x:Class="GameLibrary.AvaloniaUI.Controls.Windows.Window_Dialog" Title="Wine Settings"
    xmlns:common="clr-namespace:GameLibrary.AvaloniaUI.Controls" Background="#1E1E1E" Width="400" Height="200">
    <Grid Margin="10">
        <TextBlock TextWrapping="Wrap" x:Name="lbl_Description" Text="ttest" VerticalAlignment="Stretch" Foreground="White" HorizontalAlignment="Stretch" Margin="0,0,50,0" TextAlignment="Center"/>
        <StackPanel Orientation="Horizontal" Height="40" VerticalAlignment="Bottom" HorizontalAlignment="Right">
            <common:Common_Button x:Name="btn_Negative" Width="100" Label="Ok" Margin="0,0,5,0"/>
            <common:Common_Button x:Name="btn_Positive" Width="100" Label="Cancel" />
        </StackPanel>
    </Grid>
</Window>