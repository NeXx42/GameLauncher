<UserControl xmlns="https://github.com/avaloniaui"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="100" x:Class="GameLibrary.Avalonia.Controls.SubPage.Indexer.Indexer_Folder"
  xmlns:Common="clr-namespace:GameLibrary.Avalonia.Controls">
  <Border Background="Green" CornerRadius="5">
    <StackPanel Margin="5">
      <Border Height="40" Background="Blue">
        <Grid Margin="5">
          <Label x:Name="lbl_Extract" VerticalAlignment="Center" Content="ARCHOVE"/>
          <Common:Common_Button x:Name="btn_Extract" HorizontalAlignment="Right" Width="150" Label="Extract" />
        </Grid>
      </Border>
      <Border Height="40" Background="Red">
        <Grid Margin="5">
          <Label VerticalAlignment="Center" x:Name="lbl_Folder" />
          <Common:Common_Button x:Name="btn_Explore" HorizontalAlignment="Right" Width="150" Label="Explore" />
        </Grid>
      </Border>
    </StackPanel>
  </Border>
</UserControl>
